  "module": "dist/lmnsqzy.esm.js",
  "size-limit": [
    {
      "path": "dist/lmnsqzy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lmnsqzy.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
